{
  "name": "app-doctor-react",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio --browser none",
    "db:seed": "pnpx tsx ./prisma/seed.ts",
    "dev": "concurrently \"pnpm dev:next\" \"pnpm dev:email\"",
    "dev:next": "next dev --turbopack",
    "dev:email": "email dev --port 3001",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "zen:init": "zenstack init",
    "zen:generate": "zenstack generate",
    "zen:push": "zenstack generate && prisma db push",
    "pkgs:check": "pnpx npm-check-updates --deep --concurrency 16",
    "pkgs:update": "pnpx npm-check-updates --deep --concurrency 16 -u"
  },
