{
  "name": "app-doctor-react",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "vercel-build": "zenstack generate && next build",
    "check": "next lint && tsc --noEmit",
    "db:migrate:dev": "prisma migrate dev",
    "db:turso:migrate": "turso db shell doc-db < ./prisma/migrations/20250129011433_init/migration.sql",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio --browser none",
    "db:seed": "SEED=true pnpx tsx ./prisma/seed/seed.ts",
    "db:seed:prod": "SEED=true pnpx tsx ./prisma/seed/seed-prod.ts",
    "db:seed:supa": "SEED=true pnpx tsx ./prisma/seed/supa.ts",
    "db:seed:cid": "SEED=true pnpx tsx ./prisma/seed/cid-seed.ts",
    "w:db:seed": "cross-env SEED=true pnpx tsx ./prisma/seed/seed.ts",
    "w:db:seed:cid": "cross-env SEED=true pnpx tsx ./prisma/seed/cid-seed.ts",
    "dev": "concurrently \"pnpm dev:next\" \"pnpm dev:email\" \"pnpm db:studio\"",
    "dev:next": "next dev --turbopack",
    "dev:email": "email dev --port 3001 --dir ./src/server/email/templates",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "zen:init": "zenstack init",
    "zen:generate": "zenstack generate",
    "zen:push": "zenstack generate && prisma db push",
    "pkgs:check": "pnpx npm-check-updates --deep --concurrency 16",
    "pkgs:update": "pnpx npm-check-updates --deep --concurrency 16 -u"
  },
